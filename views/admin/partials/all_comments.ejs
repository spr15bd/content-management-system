<table class="table table-stripped">
  <tr>
    <td>ID</td>
    <td>Author</td>
    <td>Content</td>
    <td>Email</td>
    <td>Status</td>
    <td>Response to</td>
    <td>Date/td>
    <td>Approve</td>
    <td>Unapprove</td>
    <td colspan="2">Operations</td>
  </tr>
  <% 
    
  
  
  
    if(userData.length!=0) {
      Object.keys(userData).forEach((key) => {
  %>    
    <tr>
      <td><%=userData[key].comment_id%></td>
      <td><%=userData[key].comment_author%></td>
      <td><%=userData[key].comment_content%></td>
      <td><%=userData[key].comment_email%></td>
      <td><%=userData[key].comment_status%></td>
      <td><%=userData[key].comment_date%></td>
      <td><img width="50" height="50" class="img-responsive" src="<%=userData[key].post_img%>"></td>
      <td><%=userData[key].post_comment_count%></td>
      <td><%=userData[key].post_date%></td>
      <td><a href="/delete_post?del=<%=userData[key].post_id%>" class="btn btn-danger"><span class="fa fa-trash"></span></a></td>
      <td><a href="/admin/posts?edit=<%=userData[key].post_id%>&option=edit_post" class="btn btn-success"><span class="fa fa-pencil"></span></a></td>
    </tr>
  <%
      });
    }
  %>
</table> 